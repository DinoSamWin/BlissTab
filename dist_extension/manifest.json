{
    "manifest_version": 3,
    "name": "StartlyTab",
    "version": "1.0.0",
    "description": "A beautiful new tab page with gateway shortcuts.",
    "permissions": [
        "storage",
        "tabs",
        "activeTab",
        "favicon",
        "contextMenus",
        "identity"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'; connect-src 'self' https://accounts.google.com https://sudmwgwvlsdkpoxrnsji.supabase.co https://open.bigmodel.cn https://api.siliconflow.cn https://api.deepseek.com https://cloud.umami.is;"
    },
    "oauth2": {
        "client_id": "65772780936-q2m0q2jgj7b5r4vb9rn5ohm2vcvsshb7.apps.googleusercontent.com",
        "scopes": [
            "openid",
            "email",
            "profile"
        ]
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "action": {
        "default_popup": "src/extension/popup.html",
        "default_title": "Add Gateway"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+U",
                "mac": "Command+Shift+U"
            },
            "description": "Open Gateway Adder"
        }
    },
    "icons": {
        "128": "icons/icon-512x512.png"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "assets/*",
                "icons/*"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}